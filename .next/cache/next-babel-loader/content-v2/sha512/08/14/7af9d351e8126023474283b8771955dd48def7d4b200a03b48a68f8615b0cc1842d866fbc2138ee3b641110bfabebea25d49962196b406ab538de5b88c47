 </>\n  );\n}\n\nexport async function getServerSideProps () {\n  const data = await fetch('http://0.0.0.0:3000/api/reports', { method: 'GET'});\n  const { reports } = await data.json();\n\n  return {\n    props: {\n      reports\n    }\n  }\n}\n\nexport default IndexPage;"]},"metadata":{},"sourceType":"module"}